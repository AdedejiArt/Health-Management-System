                      <div class="card card-table mb-0">
                        <div class="card-body">
                          <div class="table-responsive">
                            <table class="table table-hover table-center mb-0">
                              <thead>
                                <tr>
                                  <th>Names</th>
                                  <th>Appt Date</th>

                                  <th></th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr>
                                  <td>
                                    <h2 class="table-avatar">
                                      <a href="#" class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle"
                                          src="assets/img/patients/patient.jpg" alt="User Image"
                                          name="users.fullname" /></a>
                                      <a href="#">{{users.fullname}}

                                    </h2>
                                  </td>
                                  <td>
                                    11 Nov 2019

                                  </td>

                                </tr>
                                <tr>
                                  <td>
                                    <h2 class="table-avatar">
                                      <a href="#" class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle"
                                          src="assets/img/patients/patient1.jpg" alt="User Image" /></a>
                                      <a href="#">Charlene Reed </a>
                                    </h2>
